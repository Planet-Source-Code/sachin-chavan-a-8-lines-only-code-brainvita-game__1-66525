Private Sub imgMarble_DragDrop(Index As Integer, Source As Control, X As Single, Y As Single)
If (imgMarble(Index).Picture <> 0) Or Abs(Source.Index - Index) <> 2 And Abs(Source.Index - Index) <> 20 Then Exit Sub
If (imgMarble(Source.Index - ((Source.Index - Index) / 2)).Picture = 0) Then Exit Sub
Source.Picture = LoadPicture
imgMarble(Source.Index - ((Source.Index - Index) / 2)).Picture = LoadPicture
imgMarble(Index).Picture = imgMarblepic.Picture
End Sub

